{
  "name": "quizapp",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "private": true,
  "scripts": {
    "test": "jest --watchAll --config=unit.jest.config.ts",
    "test:e2e": "jest --testPathPattern=e2e --config=e2e.jest.config.ts --runInBand",
    "build": "npm install && npx tsc -p build.tsconfig.json && npm install --prefix client && npm run build --prefix client",
    "start": "node build/start.js",
    "server": "npx tsc && DEBUG=routes:*,middleware:* nodemon build/start.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.0",
    "bcryptjs": "^2.4.3",
    "debug": "^4.1.1",
    "deep-equal": "^2.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "mongodb": "^3.5.8",
    "object.omit": "^3.0.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.45",
    "@types/jest": "^26.0.15",
    "@types/mongodb": "^3.5.20",
    "@types/node": "^14.14.7",
    "@types/supertest": "^2.0.10",
    "concurrently": "^5.2.0",
    "create-react-app": "^3.4.1",
    "jest": "^26.6.0",
    "nodemon": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.3"
  }
}
